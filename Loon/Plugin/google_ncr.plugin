[General]
force-http-engine-hosts = *.google.cn

[Rewrite]
^https?://(www.)?(g|google)\.(cn|com.hk|co.jp|com.sg|com.tw) https://www.google.com 302
^https?://books\.google\.(cn|.com.hk|co.jp|com.sg|com.tw) https://books.google.com 302
^https?://(ditu|maps)\.(g|google)\.cn https://maps.google.com 302

[MITM]

hostname = *.google.*
